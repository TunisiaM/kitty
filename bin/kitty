#!/usr/bin/env ruby

class Kitty
  def night_kitty
    night_kitty = <<-'HERE KITTY KITTY' 
               *     ,MMM8&&&.            *
                    MMMM88&&&&&    .
                   MMMM88&&&&&&&
       *           MMM88&&&&&&&&
                   MMM88&&&&&&&&
                   'MMM88&&&&&&'
                     'MMM8&&&'      *
            |\___/|
            )     (             .              '
           =\     /=
             )===(       *
            /     \
            |     |
           /       \
           \       /
    _/\_/\_/\__  _/_/\_/\_/\_/\_/\_/\_/\_/\_/\_
    |  |  |  |( (  |  |  |  |  |  |  |  |  |  |
    |  |  |  | ) ) |  |  |  |  |  |  |  |  |  |
    |  |  |  |(_(  |  |  |  |  |  |  |  |  |  |
    |  |  |  |  |  |  |  |  |  |  |  |  |  |  |
    |  |  |  |  |  |  |  |  |  |  |  |  |  |  |
    HERE KITTY KITTY
  end

  def pounce_kitty
    pounce_kitty = <<-'HERE KITTY KITTY'
     ____
    (.   \
      \  |   
       \ |___(\--/)
     __/    (  . . )
    "'._.    '-.O.'
         '-.  \ "|\
            '.,,/'.,,
    HERE KITTY KITTY
  end

  def face_kitty
    face_kitty = <<-'HERE KITTY KITTY'
          _.---.._             _.---...__
       .-'   /\   \          .'  /\     /
       `.   (  )   \        /   (  )   /
         `.  \/   .'\      /`.   \/  .'
           ``---''   )    (   ``---''
                   .';.--.;`.
                 .' /_...._\ `.
               .'   `.a  a.'   `.
              (        \/        )
               `.___..-'`-..___.'
                  \          /
                   `-.____.-' 
    HERE KITTY KITTY
  end

  def sleep_kitty
    sleep_kitty = <<-'HERE KITTY KITTY'
               __..--''``\--....___   _..,_
           _.-'    .-/";  `        ``<._  ``-+'~=.
       _.-' _..--.'_    \                    `(^) )
      ((..-'    (< _     ;_..__               ; `' 
                 `-._,_)'      ``--...____..-'
    HERE KITTY KITTY
  end

  def serious_kitty
    <<-'HERE KITTY KITTY'
                  ;,_            ,
                 _uP~"b          d"u,
                dP'   "b       ,d"  "o
               d"    , `b     d"'    "b
              l] [    " `l,  d"       lb
              Ol ?     "  "b`"=uoqo,_  "l
            ,dBb "b        "b,    `"~~TObup,_
          ,d" (db.`"         ""     "tbc,_ `~"Yuu,_
        .d" l`T'  '=                      ~     `""Yu,
      ,dO` gP,                           `u,   b,_  "b7
     d?' ,d" l,                           `"b,_ `~b  "1
   ,8i' dl   `l                 ,ggQOV",dbgq,._"  `l  lb
  .df' (O,    "             ,ggQY"~  , @@@@@d"bd~  `b "1
 .df'   `"           -=@QgpOY""     (b  @@@@P db    `Lp"b,
.d(                  _               "ko "=d_,Q`  ,_  "  "b,
Ql         .         `"qo,._          "tQo,_`""bo ;tb,    `"b,
qQ         |L           ~"QQQgggc,_.,dObc,opooO  `"~~";.   __,7,
qp         t\io,_           `~"TOOggQV""""        _,dg,_ =PIQHib.
`qp        `Q["tQQQo,_                          ,pl{QOP"'   7AFR`
  `         `tb  '""tQQQg,_             p" "b   `       .;-.`Vl'
             "Yb      `"tQOOo,__    _,edb    ` .__   /`/'|  |b;=;.__
                           `"tQQQOOOOP""`"\QV;qQObob"`-._`\_~~-._
                                """"    ._        /   | |oP"\_   ~\ ~\_~\
                                        `~"\ic,qggddOOP"|  |  ~\   `\~-._
                                          ,qP`"""|"   | `\ `;   `\   `\
                               _        _,p"     |    |   `\`;    |    |
       unknown                 "boo,._dP"       `\_  `\    `\|   `\   ;
                                 `"7tY~'            `\  `\    `|_   |
                                                      `~\  |
    HERE KITTY KITTY
  end

  def full_double_kitty
    <<-'HERE KITTY KITTY'
            *     ,MMM8&&&.            *
                  MMMM88&&&&&    .
                 MMMM88&&&&&&&
     *           MMM88&&&&&&&&
                 MMM88&&&&&&&&
                 'MMM88&&&&&&'
                   'MMM8&&&'      *
          |\___/|     /\___/\
          )     (     )    ~( .              '
         =\     /=   =\~    /=
           )===(       ) ~ (
          /     \     /     \
          |     |     ) ~   (
         /       \   /     ~ \
         \       /   \~     ~/
  jgs_/\_/\__  _/_/\_/\__~__/_/\_/\_/\_/\_/\_
  |  |  |  |( (  |  |  | ))  |  |  |  |  |  |
  |  |  |  | ) ) |  |  |//|  |  |  |  |  |  |
  |  |  |  |(_(  |  |  (( |  |  |  |  |  |  |
  |  |  |  |  |  |  |  |\)|  |  |  |  |  |  |
  |  |  |  |  |  |  |  |  |  |  |  |  |  |  |
    HERE KITTY KITTY
  end

  def chub_kitty
    chub_kitty = <<-'HERE KITTY KITTY'
                .                .                    
                :"-.          .-";                    
                |:`.`.__..__.'.';|                    
                || :-"      "-; ||                    
                :;              :;                    
                /  .==.    .==.  \                    
               :      _.--._      ;                   
               ; .--.' `--' `.--. :                   
              :   __;`      ':__   ;                  
              ;  '  '-._:;_.-'  '  :                  
              '.       `--'       .'                  
               ."-._          _.-".                   
             .'     ""------""     `.                 
            /`-                    -'\                
           /`-                      -'\               
          :`-   .'              `.   -';              
          ;    /                  \    :              
         :    :                    ;    ;             
         ;    ;                    :    :             
         ':_:.'                    '.;_;'             
            :_                      _;                
            ; "-._                -" :`-.     _.._    
            :_          ()          _;   "--::__. `.  
             \"-                  -"/`._           :  
            .-"-.                 -"-.  ""--..____.'  
           /         .__  __.         \               
          : / ,       / "" \       . \ ;
           "-:___..--"      "--..___;-"    
    HERE KITTY KITTY
  end

  def tiny_kitty
    <<-'HERE KITTY KITTY'

    )   _. mmeeoowwrr!
   (___)''
   / ,_,/
  /'"\ )\

itz
    HERE KITTY KITTY
  end

  def hungry_kitty
    <<-'HERE KITTY KITTY'
                                _.---.
                      |\---/|  / ) ca|
          ------------;     |-/ /|foo|---
                      )     (' / `---'
          ===========(       ,'==========
          ||   _     |      |
          || o/ )    |      | o
          || ( (    /       ;
          ||  \ `._/       /
          ||   `._        /|
          ||      |\    _/||
        __||_____.' )  |__||____________
         ________\  |  |_________________
                  \ \  `-.
                   `-`---'  hjw
    HERE KITTY KITTY
  end

  def curious_kitty
    curious_kitty = <<-'HERE KITTY KITTY'
                    _                        
                    \`*-.                    
                     )  _`-.                 
                    .  : `. .                
                    : _   '  \               
                    ; *` _.   `*-._          
                    `-.-'          `-.       
                      ;       `       `.     
                      :.       .        \    
                      . \  .   :   .-'   .   
                      '  `+.;  ;  '      :   
                      :  '  |    ;       ;-. 
                      ; '   : :`-:     _.`* ;
                   .*' /  .*' ; .*`- +'  `*' 
                   `*-*   `*-*  `*-*'        
    HERE KITTY KITTY
  end

  def proud_kitty
    proud_kitty = <<-'HERE KITTY KITTY'
                           ;\
                          _' \_
                        ,' '  '`.
                       ;,)       \
                      /          :
                      (_         :
                       `--.       \
                          /        `.
                         ;           `.
                        /              `.
                       :                 `.
                       :                   \
                        \\                  \
                         ::                 :
                         || |               |
                         || |`._            ;
                        _;; ; __`._,       (________
                      ((__/(_____(______,'______(___)
    HERE KITTY KITTY
  end

  def stretch_kitty
    stretch_kitty = <<-'HERE KITTY KITTY'
            _,'|             _.-''``-...___..--';)
           /_ \'.      __..-' ,      ,--...--'''
          <\    .`--'''       `     /'
           `-';'               ;   ; ;
     __...--''     ___...--_..'  .;.'
    (,__....----'''       (,..--''
    HERE KITTY KITTY
  end

  def tail_kitty
    tail_kitty = <<-'HERE KITTY KITTY'
                                 _
                                | \
                                | |
                                | |
           |\                   | |
          /, ~\                / /
         X     `-.....-------./ /
          ~-. ~  ~              |
             \             /    |
              \  /_     ___\   /
              | /\ ~~~~~   \ |
              | | \        || |
              | |\ \       || )
             (_/ (_/      ((_/
    
    HERE KITTY KITTY
  end

  def scared_kitty
    scared_kitty = <<-'HERE KITTY KITTY'
         /\_/\
    /\  / o o \
   //\\ \~(*)~/
   `  \/   ^ /
      | \|| ||  
      \ '|| ||  
       \)()-())
    HERE KITTY KITTY
  end

  def dumb_kitty
    dumb_kitty = <<-'HERE KITTY KITTY'
       /\     /\
      {  `---'  }
      {  O   O  }  
    ~~|~   V   ~|~~  
       \  \|/  /   
        `-----'__
        /     \  `^\_
       {       }\ |\_\_   W
       |  \_/  |/ /  \_\_( )
        \__/  /(_E     \__/
          (  /
           MM
    HERE KITTY KITTY
  end

  def crazy_kitty
    crazy_kitty = <<-'HERE KITTY KITTY'
                 ._
              .-'  `-.
           .-'        \
          ;    .-'\    ;
          `._.'    ;   |
                   |   |
                   ;   :
                  ;   :
                  ;   :
                 /   /
                ;   :                   ,
                ;   |               .-"7|
              .-'"  :            .-' .' :
           .-'       \         .'  .'   `.
         .'           `-. ""-.-'`""    `",`-._..--"7
         ;    .          `-.J `-,    ;"`.;|,_,    ;
       _.'    |         `"" `. ."""--. o \:.-. _.'
    .""       :            ,--`;   ,  `--/}o,' ;
    ;   .___.'        /     ,--.`-. `-..7_.-  /_
     \   :   `..__.._;    .'__;    `---..__.-'-.`"-,
     .'   `--. |   \_;    \'   `-._.-")     \\  `-,
     `.   -.`_):      `.   `-"""`.   ;__.' ;/ ;   "
       `-.__7"  `-..._.'`7     -._;'  ``"-''
                         `--.,__.'
    HERE KITTY KITTY
  end

  def dominant_kitty
    dominant_kitty = <<-'HERE KITTY KITTY'
            /\
            \ \
             \ \
             / /
            / /
           _\ \_/\/\
          /  *  \@@ =
         |       |Y/
         |       |~
          \ /_\ /
           \\ //
            |||
           _|||_
          ( / \ )
    HERE KITTY KITTY
  end

  def all_kitties
    methods.select { |m| m =~ /_kitty$/ }
  end

  def random
    print send(all_kitties[rand(all_kitties.length)])
  end
end

require 'optparse'

options = { :count => 1 }
OptionParser.new do |opts|
  opts.banner = "Usage #{__FILE__} [options]"
  opts.on("-c N", "--count N", "How many kitties do you want?") do |count|
    options[:count] = count.to_i
  end

  opts.on_tail("-h", "--help", "Show this message") do
    puts opts
    exit
  end
end.parse!

kitty = Kitty.new
options[:count].times do
  kitty.random
end

